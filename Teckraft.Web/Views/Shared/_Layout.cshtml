                        <li class="dropdown user user-menu">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/Content/img/avatar6.png" class="user-image" alt="User Image" />
                                <span class="hidden-xs">@ViewBag.currentUserTitle</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="~/Content/img/avatar6.png" class="img-circle" alt="User Image" />
